{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"My Team PUBG",
		"version":"0.0.2",
		"minimum-overwolf-version":"0.120.0",
		"author":"MyTeam",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Primeira App"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"dist/index.html",
					"show_in_taskbar": false,
					"transparent": true,
					"show_minimize": false,
					"show_maximize": false,
					"resizable": true,
					"use_os_windowing": false,
					"size":{"width":839, "height":839},
					"min_size":{"width":1300, "height":839},
					"max_size":{"width":1920, "height":1040}
				} 
		}
	}
}
